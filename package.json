{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "rateyourmusic",
  "title": "rateyourmusic",
  "description": "What is the review of this album in RateYourMusic?",
  "icon": "command-icon.png",
  "author": "leepyd",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search on RateYourMusic",
      "description": "Opens RateYourMusic with your search query in the URL",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}